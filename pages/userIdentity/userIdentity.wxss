/* pages/identity/identity.wxss */
.page-container {
  padding: 40rpx;
}

.form-group {
  margin-bottom: 20px;
}

.label {
  font-size: 16px;
  margin-bottom: 20rpx;
}

.input {
  width: 635rpx;
  padding: 10px;
  border: 1px solid #ccc;
  border-radius: 4px;
  margin-top: 15rpx;
}

.submit-button {
  width: 100%;
  padding: 4px;
  background-color: #ff8c00;
  color: #ffffff;
  border: none;
  border-radius: 4px;
  font-size: 17px;
  letter-spacing: 5rpx;
}
